{
  "name": "@galxe-identity-protocol/evm-contracts",
  "version": "1.0.2",
  "description": "Galxe Identity Protocol EVM Contracts",
  "packageManager": "pnpm@9.1.0+sha256.22e36fba7f4880ecf749a5ca128b8435da085ecd49575e7fb9e64d6bf4fad394",
  "homepage": "https://www.galxe.com/",
  "license": "MIT",
  "types": "dist/typechain-types/index.ts",
  "main": "dist/typechain-types/index.ts",
  "files": [
    "contracts/",
    "dist/"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm compile && pnpm typechain && pnpm export-types",
    "lint": "pnpm _lint",
    "_lint": "pnpm lint:sol && pnpm lint:ts && pnpm prettier:check",
    "test": "hardhat test",
    "clean": "rimraf ./artifacts ./cache ./dist ./coverage ./types ./coverage.json",
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "coverage": "hardhat coverage --solcoverjs ./.solcover.js --temp build/artifacts --testfiles \"test/**/*.ts\"",
    "lint:sol": "solhint \"contracts/**/*.sol\"",
    "lint:ts": "eslint --ignore-path ./.eslintignore --ext .js,.ts .",
    "postinstall": "DOTENV_CONFIG_PATH=./.env",
    "prettier:check": "prettier --check \"**/*.{js,json,md,sol,ts,yml}\"",
    "prettier:write": "prettier --write \"**/*.{js,json,md,sol,ts,yml}\"",
    "test-gas": "REPORT_GAS=true hardhat test",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain",
    "export-types": "copyfiles -u 1 \"typechain-types/**/*.ts\" dist/typechain-types",
    "deploy:sepolia": "hardhat deploy --tags GalxeIdentityProtocol --network sepolia",
    "deploy-primitive:sepolia": "hardhat deploy --tags BabyZKPrimitive --network sepolia",
    "verify:sepolia": "NETWORK=sepolia hardhat etherscan-verify --network sepolia",
    "register-primitive:sepolia": "hardhat run scripts/registerPrimitiveTypes.ts --network sepolia",
    "deploy:bsc": "hardhat deploy --tags GalxeIdentityProtocol --network bsc",
    "deploy-primitive:bsc": "hardhat deploy --tags BabyZKPrimitive --network bsc",
    "verify:bsc": "NETWORK=bsc hardhat etherscan-verify --network bsc",
    "register-primitive:bsc": "hardhat run scripts/registerPrimitiveTypes.ts --network bsc",
    "deploy:polygon": "hardhat deploy --tags GalxeIdentityProtocol --network polygon",
    "deploy-primitive:polygon": "hardhat deploy --tags BabyZKPrimitive --network polygon",
    "verify:polygon": "NETWORK=polygon hardhat etherscan-verify --network polygon",
    "register-primitive:polygon": "hardhat run scripts/registerPrimitiveTypes.ts --network polygon",
    "deploy:mainnet": "hardhat deploy --tags GalxeIdentityProtocol --network mainnet",
    "deploy-primitive:mainnet": "hardhat deploy --tags BabyZKPrimitive --network mainnet",
    "verify:mainnet": "NETWORK=mainnet hardhat etherscan-verify --network mainnet",
    "register-primitive:mainnet": "hardhat run scripts/registerPrimitiveTypes.ts --network mainnet",
    "deploy:gravity": "hardhat deploy --tags GalxeIdentityProtocol --network gravity",
    "deploy-primitive:gravity": "hardhat deploy --tags BabyZKPrimitive --network gravity",
    "verify:gravity": "NETWORK=gravity hardhat etherscan-verify --network gravity",
    "register-primitive:gravity": "hardhat run scripts/registerPrimitiveTypes.ts --network gravity"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.6",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.10",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.1.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@typechain/ethers-v6": "^0.4.3",
    "@typechain/hardhat": "^8.0.3",
    "@types/chai": "^4.3.16",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.33",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "chai": "^4.4.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "fs-extra": "^10.1.0",
    "hardhat-deploy": "^0.12.4",
    "hardhat-gas-reporter": "^1.0.10",
    "lodash": "^4.17.21",
    "mocha": "^10.4.0",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.3.1",
    "rimraf": "^4.4.1",
    "solhint": "^3.6.2",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.8.12",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.6",
    "ethereumjs-util": "^7.1.5",
    "ethers": "^6.12.1",
    "hardhat": "^2.22.3"
  }
}
